<launch4jConfig>
  <headerType>0</headerType>
  <jar>../build/tonic.jar</jar>
  <outfile>../build/Tonic.exe</outfile>
  <errTitle>Tonic</errTitle>
  <chdir>.</chdir>
  <customProcName>true</customProcName>
  <icon>Tonic.ico</icon>
  <jre>
    <minVersion>1.4.0</minVersion>
  </jre>
  <splash>
    <file>tonic.bmp</file>
    <waitForWindow>true</waitForWindow>
    <timeout>60</timeout>
    <timeoutErr>true</timeoutErr>
  </splash>
</launch4jConfig>